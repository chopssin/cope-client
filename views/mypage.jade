extends layout

block content
  #page-container.purely
    #nav.nav-top.cope
      .container
        #logo.logo.text
          | #{title}
    #page.page-wrap.cope
      #dashboard.container
        .row
          .col-xs-12.col-md-4.col-md-push-8
            h4 Account
            .row
              .col-xs-12
                #account.cope-card.bg-w.
          .col-xs-12.col-md-8.col-md-pull-4
            h4 Apps
            .row
              #my-apps.col-xs-12
              .col-xs-12
                button#btn-add-app.cope-btn(style="margin: 16px 0")
                  h4 Add new app
      #dashboard-app.container.hidden
        .row
          #app-page.col-xs-12
             
block javascripts
  script( src = "https://d3js.org/d3.v4.min.js" )
  script( src = "javascripts/views.js" )
  script( src = "javascripts/test-cope.js" )
  script( src = "javascripts/cope-home.js" )
